{
  "name": "gemini-code-showcase",
  "displayName": "Gemini Code Showcase",
  "description": "Suggest and apply skill documents for Gemini-based AI workflows.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "20.x",
    "@types/vscode": "1.94.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "gemini.applySkill",
        "title": "Gemini: Apply Skill"
      },
      {
        "command": "gemini.listSkills",
        "title": "Gemini: List Skills"
      },
      {
        "command": "gemini.sendToModel",
        "title": "Gemini: Send Prompt to Model (requires API key)"
      }
    ]
  }
}
